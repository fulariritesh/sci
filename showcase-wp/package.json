{
  "name": "underscores",
  "version": "1.0.0",
  "description": "Hi. I'm a starter theme called _s, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "@popperjs/core": "^2.6.0",
    "@splidejs/splide": "^2.4.21",
    "@wordpress/scripts": "^12.1.0",
    "jquery": "^3.5.1",
    "laravel-mix": "^6.0.2",
    "laravel-mix-purgecss": "^6.0.0",
    "lightbox2": "^2.11.3",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.14",
    "purgecss": "^3.1.3",
    "purgecss-with-wordpress": "^3.1.3",
    "resolve-url-loader": "^3.1.0",
    "rtlcss": "^2.5.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "build": "npx mix --production",
    "watch": "node-sass sass/ -o ./dist --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "compile:css": "node-sass sass/ -o ./dist && stylelint ./dist/main.css --fix || true && stylelint ./dist/main.css --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style ./sass/*.scss --fix",
    "lint:js": "wp-scripts lint-js ./js/*.js"
  },
  "dependencies": {
    "@types/jqueryui": "^1.12.14"
  }
}
